{
  "name": "@roomy-chat/sdk",
  "version": "0.2.0",
  "license": "MPL-2.0",
  "type": "module",
  "packageManager": "pnpm@10.10.0",
  "repository": {
    "url": "https://github.com/muni-town/roomy-sdk"
  },
  "files": [
    "./src",
    "./dist",
    "./README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && cp src/utils/spow/spow-wasm_bg.wasm dist/utils/spow/",
    "format": "prettier . --write",
    "build-lib": "pnpm build"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./utils/invites": {
      "development": "./src/utils/invites.ts",
      "import": "./dist/utils/invites.js"
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@repo/tsconfig": "workspace:*",
    "beachball": "^2.54.0",
    "jazz-tools": "catalog:",
    "jazz-run": "^0.17.2",
    "pkgroll": "^2.12.2",
    "prettier": "3.5.3",
    "tsx": "^4.20.4",
    "turbo": "^2.5.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/node": "^24.2.1"
  }
}
