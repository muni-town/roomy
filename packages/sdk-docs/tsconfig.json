{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "exactOptionalPropertyTypes": true,
    "strictNullChecks": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": [
      "deno",
    ]
  },
  "include": [
    "../sdk/src"
  ],
}