{
  "name": "discord-bridge",
  "version": "1.0.0",
  "description": "Bridge between Discord and Roomy - Node.js Server",
  "main": "dist/server.js",
  "type": "module",  "scripts": {
    "dev": "nodemon -I --exec node --experimental-specifier-resolution=node --loader ts-node/esm ./src/server.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "discord:dev": "nodemon -I --exec node --experimental-specifier-resolution=node  --loader ts-node/esm ./src/index.ts"
  },  "dependencies": {
    "@roomy-chat/sdk": "workspace:*",
    "cors": "^2.8.5",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "undici": "6.21.1",
    "ws": "^8.18.2"
  },  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.17",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}